  <connectionStrings configSource="connectionStrings.config" />
  <appSettings configSource="appSettings.config">
  </appSettings>
  <system.web>
    <customErrors mode="Off" defaultRedirect="/ComingSoon">
      <error statusCode="500" redirect="/ComingSoon" />
      <error statusCode="404" redirect="/ComingSoon" />
      <error statusCode="401" redirect="/ComingSoon" />
    </customErrors>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" maxRequestLength="52428800" />
    <roleManager enabled="true" />

    <hostingEnvironment shadowCopyBinAssemblies="true" />
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules>
  </system.web>
  <location path="Administration">
    <system.web>
      <customErrors mode="Off" defaultRedirect="/Administration/Errors/HttpError">
        <error statusCode="500" redirect="/Administration/Errors/Http500" />
        <error statusCode="404" redirect="/Administration/Errors/Http404" />
        <error statusCode="401" redirect="/Administration/Errors/Http401" />
      </customErrors>
      <httpRuntime maxRequestLength="52428800" />
    </system.web>
    <system.webServer>
      <httpErrors errorMode="Custom">
        <remove statusCode="404" subStatusCode="-1" />
        <error statusCode="404" path="/Administration/Errors/Http404" responseMode="ExecuteURL" />
        <remove statusCode="500" subStatusCode="-1" />
        <error statusCode="500" path="/Administration/Errors/Http500" responseMode="ExecuteURL" />
        <remove statusCode="401" subStatusCode="-1" />
        <error statusCode="401" path="/Administration/Errors/Http401" responseMode="ExecuteURL" />
      </httpErrors>
    </system.webServer>
  </location>
  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff" />
    </staticContent>
    <modules runAllManagedModulesForAllRequests="true">
      <remove name="FormsAuthenticationModule" />
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" preCondition="managedHandler" />
    </modules>
    <httpErrors errorMode="Custom">
      <remove statusCode="404" subStatusCode="-1" />
      <error statusCode="404" path="/ComingSoon" responseMode="ExecuteURL" />
      <remove statusCode="500" subStatusCode="-1" />
      <error statusCode="500" path="/ComingSoon" responseMode="ExecuteURL" />
      <remove statusCode="401" subStatusCode="-1" />
      <error statusCode="401" path="/ComingSoon" responseMode="ExecuteURL" />
    </httpErrors>
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
